<MudDialog>
    <DialogContent>
        <MudText>@QuestionText</MudText>
    </DialogContent>
    <DialogActions>
        <MudButton OnClick="Cancel">@CancelButtonText</MudButton>
        <MudButton Color="@Color" Variant="Variant.Filled" OnClick="Proceed">@ProceedButtonText</MudButton>
    </DialogActions>
</MudDialog>